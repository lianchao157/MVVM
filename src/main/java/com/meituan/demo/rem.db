求和

select sum(age) as '年龄总和' from dbo.TEST;

最大值

select max(age) as '最大值' from dbo.TEST;

最小值

select min(age) as '最小值' from dbo.TEST;

清除重复值

select   distinct address  from dbo.TEST;



